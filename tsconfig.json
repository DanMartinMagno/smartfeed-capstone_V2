{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*", "types/*"]
      }
    },
    "include": ["backend/**/*.ts", "frontend/**/*.ts", "frontend/**/*.tsx", "frontend/declarations.d.ts"],
    "exclude": ["node_modules", "dist"]
  }
  